{
  "manifest_version": 2,
  "name": "What's New on GitHub",
  "version": "1.0.2",

  "description": "See what's new and what's not in your GitHub home feed.",
  "icons": {
    "48": "icon/48.png",
    "128": "icon/128.png"
  },

  "content_scripts": [{
    "matches": ["https://github.com/"],
    "js": ["index.js"],
    "run_at": "document_end"
  }]
}
